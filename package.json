{
  "name": "react-rest-playground",
  "version": "0.1.6",
  "description": "Rest Client Playground using React",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "webpack serve --mode development",
    "clean": "rm -rf node_modules",
    "build:app": "webpack --mode production --progress",
    "build": "babel src -d dist --copy-files",
    "prettier": "npx prettier --check src/**/*.js",
    "build:declaration": "npx -p typescript tsc src/**/*.js --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "keywords": [
    "react",
    "components",
    "postman-like-tool",
    "rest-playground"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramsunvtech/react-rest-playground"
  },
  "author": {
    "name": "Venkat.R",
    "url": "https://www.linkedin.com/in/venkat-engineering-manager/"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0",
    "prop-types": "^15.8.0",
    "qs": "^6.10.2",
    "react-ace": "^9.5.0",
    "react-hook-form": "^7.22.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@svgr/webpack": "^6.1.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "copy-webpack-plugin": "^10.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.1"
  }
}
